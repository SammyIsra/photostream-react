{
  "name": "react-photostream",
  "version": "1.2.0-beta.1",
  "description": "Simple photo gallery display to be seen as a continuous stream of images, no space inbetween.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "build:demo": "nwb build-demo",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo --port 5000",
    "serve": "nwb serve --port 5000",
    "deploy": "npm publish",
    "info": "npm info react-photostream",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "deploy:surge": "mv demo/dist/index.html demo/dist/200.html&&surge --project ./demo/dist --domain react-photostream.surge.sh"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "enzyme": "2.8.2",
    "nwb": "0.16.3",
    "react": "15.5.4",
    "react-addons-test-utils": "15.5.1",
    "react-dom": "15.5.4",
    "react-router-dom": "4.1.1",
    "react-test-renderer": "15.5.4",
    "surge": "0.19.0",
    "whatwg-fetch": "2.0.3"
  },
  "author": "Sammy Israwi <sammy.israwi@gmail.com>",
  "homepage": "https://github.com/SammyIsra/photostream-react",
  "license": "MIT",
  "repository": "https://github.com/SammyIsra/photostream-react",
  "keywords": [
    "react-component",
    "react",
    "photo wall",
    "photo stream",
    "photo collage",
    "photo gallery",
    "gallery"
  ]
}
